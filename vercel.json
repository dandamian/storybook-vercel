{
  "name": "storybook-now",
  "alias": ["storybook-vercel", "storybook-now", "storybook"],
  "public": true,
  "version": 2,
  "builds": [{ "src": "storybook-static/*", "use": "@now/static" }],
  "routes": [{ "src": "/(.*)", "dest": "storybook-static/$1" }],
  "github": {
    "enabled": false
  }
}
